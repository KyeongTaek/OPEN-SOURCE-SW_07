void 달력년도(int year,int month)
{
	printf("■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
	printf("■■■■■■■■■■                  달력 선택창                 ■■■■■■■■■■■\n");
	printf("■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
	printf("■■■                    ■■■■                    ■■■■                    ■■■\n");
	printf("■■■      %d년%d월    ■■■■      ??년??월      ■■■■      ■■■■      ■■■\n",year,month);
	printf("■■■     ┌─┬─┬─┬─┐      ■■■■     ┌─┬─┬─┬─┐      ■■■■     ■      ■     ■■■\n");
	printf("■■■     ├─┼─┼─┼─┤      ■■■■     ├─┼─┼─┼─┤      ■■■■    ■     ■ ■    ■■■\n");
	printf("■■■     ├─┼─┼─┼─┤      ■■■■     ├─┼─┼─┼─┤      ■■■■     ■      ■     ■■■\n");
	printf("■■■     └─┴─┴─┴─┘      ■■■■     └─┴─┴─┴─┘      ■■■■      ■■■■ ■   ■■■\n");
	printf("■■■                    ■■■■                    ■■■■                    ■■■\n");
