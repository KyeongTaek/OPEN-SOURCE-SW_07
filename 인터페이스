	printf("■■■   ■■■■         ■■■■  └──────────────┘  ■■■■     └─┴─┴──┴─┘     ■■■■      ■■■■ ■   ■■■\n");
	printf("■■■                    ■■■■                    ■■■■                    ■■■■                    ■■■\n");
	printf("■■■ 1.시험 D-DAY 확인  ■■■■ 2.저장 데이터 보기 ■■■■  3.달력 시험 확인  ■■■■     4^.뒤로가기    ■■■\n");
	printf("■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
	printf("■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
	printf("■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
	printf("입력:");
}
void 시험날짜()
{
	system("cls");
	printf("\n\n\n\n\n\t\t\t■■■■■■■■■■■■<<★ㅇ□ㅇ★>>■■■■■■■■■■■■\n");
	printf("\t\t\t■■■■■■■ 시험 날짜를 입력해주세요(ex:2021-12-09):");
}
void 시험이름()
{
	system("cls");
	printf("\n\n\n\n\n\t\t\t■■■■■■■■■■■■<<★ㅇ□ㅇ★>>■■■■■■■■■■■■\n");
	printf("\t\t\t■■■■■■■ 시험 이름을 입력해주세요(ex:오픈소스SW시험):");
}
void 저장된시험(int dyear,int dmonth,int dday,char frs[MAX])
{
	printf("\n ■■■현재 저장된 시험■■■\n");
	printf("┌───────────────────────────────────────────────────────────┐\n");
	printf("│%s까지 %d년 %d월 %d일 남았습니다.\n",frs,dyear,dmonth,dday);
	printf("└───────────────────────────────────────────────────────────┘\n");
}
