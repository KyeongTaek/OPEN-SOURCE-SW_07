		printf("시험 d-dya(press '3')\n");
		printf("끝내기(press '9')\n");
		scanf_s("%d", &select);
		if (select == 1)
		{
			dday();
		}
		else if (select == 2)
		{
			calender();
		}
		else if (select == 3)
		{

		}
	}

}
